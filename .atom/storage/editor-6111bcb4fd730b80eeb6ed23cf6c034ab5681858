@available_collection ||= @options[:return_models] ? available_tax_rates : load_dto_collection\n    end\n\n    # returns tax rates that are available for a certain period\n    # Filters tax_rates by visibility by default\n    # Set filtering_date\n    # @return [Collection<DTO::TaxRate>]\n    # TODO: VICMAR TAX_REFACT this methods needs to disapear and only the available should exist.\n    # The available method needs to return tax rates with the updated percentage.\n    def available_with_filtering_date\n      available_tax_rates.each do |tax_rate|\n        tax_rate.apply_filtering_date(@date)\n      end\n    end\n\n    # Returns all available tax rates for the current scope_id without any additional filtering.\n    # This method is useful to make custom filtering a possibility and for tax management interfaces.\n    #\n    # @return [CoreAccounting::Taxes::Models::TaxRate]\n    def all_available_tax_rates_for_scope\n      @all_available_tax_rates_for_scope ||= Models::TaxRate.available(@scope_id, @date)\n    end\n\n    # returns a DTO object for a given tax rate\n    #\n    # @param tax_rate_id [Integer]\n    # @return [DTO::TaxRate]\n    def tax_rate_for_id(tax_rate_id)\n      trp = CoreAccounting::Taxes::Models::TaxRatePercentage.tax_rate_percentage_for_tax_rate_id(tax_rate_id, @date)\n      if trp.present?\n        return_tax_rate(tax_rate_obj: trp)\n      else\n        tax_rate = Models::TaxRate.find_by_id(tax_rate_id)\n        tax_rate.apply_filtering_date(@date) if tax_rate\n        return tax_rate if @options[:return_models]\n        if tax_rate\n          if tax_rate.combined?\n            return_tax_rate(tax_rate_obj: tax_rate,\n                            calculated_percentage: calculate_combined_tax_rate(tax_rate),\n                            sub_tax_rates: get_sub_tax_rates_for(tax_rate))\n          # else\n            # Expired tax rate\n          end\n        end\n      end\n    end\n\n    # returns the rate percentage for a given tax rate id for a certain date.\n    # It will return nil if tax rate is expired or tax rate does not have a percentage\n    #\n    # @param tax_rate_id [Integer]\n    # @return [BigDecimal] tax rate percentage\n    def percentage_for_tax_rate_id(tax_rate_id)\n      tax_rate = Models::TaxRate.find_by_id(tax_rate_id)\n      tax_rate.percentage_for_date(@date) if tax_rate\n    end\n\n    # Get the control name which represents the ledger account for posting\n    # @return [Symbol] the control name which represents the ledger account for postings\n    def control_name_for_tax_rate(key, tax_rate_id)\n      @associations ||= CoreAccounting::Taxes::Models::CoaAccountTaxRateAssociation.all.pluck(:tax_rate_id,\n                                                                                              :sales_control_name,\n                                                                                              :purchase_control_name)\n\n\n      index = key == :sales ? CoreAccounting::Taxes::Models::CoaAccountTaxRateAssociation::SALES_CONTROL_INDEX :\n                              CoreAccounting::Taxes::Models::CoaAccountTaxRateAssociation::PURCHASE_CONTROL_INDEX\n\n      if tax_rate_id.present?\n        control_name = @associations.detect { |a| a[0] == tax_rate_id}[index]\n        control_name ? control_name.to_sym : control_name\n      end\n    end\n\n    # Determine whether rate is Tax Exempt\n    #\n    # @param  rate_id [Integer] Tax rate ID\n    # @return [Boolean]\n    def exempt_rate?(rate_id)\n      rate_id == exempt_tax_rate.try(:id)\n    end\n\n    private\n\n    attr_reader :business, :state_id\n    attr_reader :tax_profiles_provider\n\n    def defaults\n      @options ||= {\n        filter_visible: true, # ignore visibility flags\n        return_models:  false, # return models instead of DTOs\n      }\n    end\n\n    def load_dto_collection\n      DTO::DtoCollection.new.tap do |collection|\n        available_tax_rates.each do |tax_rate|\n          tax_rate_dto = if tax_rate.combined?\n                           load_dto_from_combined_tax_rate(tax_rate)\n                         else\n                           load_dto_from_tax_rate_percentage(tax_rate)\n                         end\n\n          collection << tax_rate_dto\n        end\n      end\n    end\n\n    def load_dto_from_combined_tax_rate(combined_tax_rate)\n      return_tax_rate(tax_rate_obj: combined_tax_rate,\n                      calculated_percentage: calculate_combined_tax_rate(combined_tax_rate),\n                      sub_tax_rates: get_sub_tax_rates_for(combined_tax_rate))\n    end\n\n    def load_dto_from_tax_rate_percentage(tax_rate)\n      tax_rate_percentage = tax_rate.tax_rate_percentages.first # The provider should assure that the first percentage is the latest\n      return_tax_rate(tax_rate_obj: tax_rate_percentage)\n    end\n\n    def available_tax_rates\n      context = {state_id: state_id, tax_system: tax_system, business: business}\n      available_tax_filter_klass.new(self, options: @options, context: context).filter_available_tax_rates\n    end\n\n    def calculate_combined_tax_rate(tax_rate)\n      raise 'Expected a combined tax rate' unless tax_rate.combined?\n      tax_rate.sub_tax_rates.inject(BigDecimal.new('0.00')) do |total, sub_tax_rate|\n        percentage = percentage_for_tax_rate_id(sub_tax_rate.id) || BigDecimal.new('0.00')\n        total+percentage\n      end\n    end\n\n    def get_sub_tax_rates_for(combined_tax_rate)\n      DTO::DtoCollection.new.tap do |collection|\n        combined_tax_rate.sub_tax_rates.each do |sub_tax_rate|\n          tpr = CoreAccounting::Taxes::Models::TaxRatePercentage.tax_rate_percentage_for_tax_rate_id(sub_tax_rate, @date)\n          collection << DTO::TaxRate.new(tpr) unless tpr.nil? #ignore expired sub_tax_rates\n        end\n      end\n    end\n\n    def return_tax_rate(tax_rate_obj:, calculated_percentage: nil, sub_tax_rates: nil)\n      set_filter_date_to_tax_rate_obj(tax_rate_obj)\n      if @options[:return_models]\n        tax_rate_obj.tax_rate\n      else\n        DTO::TaxRate.new(tax_rate_obj, calculated_percentage, sub_tax_rates)\n      end\n    end\n\n    def set_filter_date_to_tax_rate_obj(tax_rate_obj)\n      if tax_rate_obj.respond_to?(:apply_filtering_date)\n        tax_rate_obj.apply_filtering_date(@date)\n      else\n        tax_rate_obj.tax_rate.apply_filtering_date(@date)\n      end\n    end\n\n    def scope_id\n      tax_system.scope_id\n    end\n\n    def available_tax_filter_klass\n      return StatesTaxFilter if tax_system.provincial_tax_enabled?\n      GenericTaxFilter\n    end\n\n    def tax_finder\n      Helpers::TaxFinder.new(tax_rates: all_available_tax_rates_for_scope)\n    end\n\n    def vat_tax_finder\n      Helpers::VatTaxFinder.new(tax_rates: all_available_tax_rates_for_scope)\n    end\n\n    def tax_system\n      GlobalAccountingBase::Handlers::TaxSystems.obtain(business)\n    end\n  end\nend\n","defaultMarkerLayerId":"0","markerLayers":{"0":{"id":"0","maintainHistory":false,"markersById":{"2":{"range":{"start":{"row":0,"column":0},"end":{"row":0,"column":6}},"properties":{},"reversed":false,"tailed":true,"valid":true,"invalidate":"inside"},"3":{"range":{"start":{"row":8,"column":2},"end":{"row":8,"column":7}},"properties":{},"reversed":false,"tailed":true,"valid":true,"invalidate":"inside"},"4":{"range":{"start":{"row":177,"column":37},"end":{"row":177,"column":38}},"properties":{},"reversed":false,"tailed":true,"valid":true,"invalidate":"inside"},"5":{"range":{"start":{"row":177,"column":62},"end":{"row":177,"column":65}},"properties":{},"reversed":false,"tailed":true,"valid":true,"invalidate":"inside"}},"version":2},"1":{"id":"1","maintainHistory":false,"markersById":{},"version":2},"2":{"id":"2","maintainHistory":true,"markersById":{"1":{"range":{"start":{"row":177,"column":63},"end":{"row":177,"column":63}},"properties":{"type":"selection","goalScreenRange":null},"reversed":false,"tailed":false,"valid":true,"invalidate":"never"}},"ve