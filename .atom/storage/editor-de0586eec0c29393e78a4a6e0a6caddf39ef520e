 that a mixin is used\n    def self.sanity_ensure_mixin_used(model, mixin)\n      object = self.send(model)\n      unless object.ancestors.include? mixin\n        raise Exceptions::Engine::MixinMissingException.new object, mixin\n      end\n    end\n\n    # @param identifier [Symbol] callback to register\n    # @param block [Block] block for given callback\n    #\n    # Register Callbacks\n    def self.register_callback(identifier, &block)\n      @registered_callbacks ||= {}\n      @registered_callbacks[identifier] ||= []\n      @registered_callbacks[identifier] << block\n    end\n\n    # @param identifier [Symbol] callback to trigger\n    # @param args [Hash] additional options\n    #\n    # Trigger a callback\n    def self.trigger_callback(identifier, *args)\n      @registered_callbacks ||= {}\n      @registered_callbacks[identifier] ||= []\n      @registered_callbacks[identifier].each {|c| c.call *args}\n    end\n\n    # @param subject\n    # @param params [Hash] defaults to empty hash\n    # Subject is the 'owner' of the ledgers being manipulated, i.e. business\n    def self.with(subject, params = {})\n      # sanity_check\n      unless block_given?\n        raise Exceptions::Engine::BlockRequired.new \"with\"\n      end\n      begin\n        if params[:ruleset]\n          old_ruleset = self.ruleset\n          set_ruleset params[:ruleset]\n        end\n        old_subject = self.subject\n        set_subject subject\n        yield\n      ensure\n        if old_ruleset\n          set_ruleset old_ruleset\n        else\n          clear_ruleset\n        end\n        set_subject old_subject\n      end\n    end\n\n    # Ruleset\n    def self.ruleset\n      Thread.current[:accounts_processor_ruleset]\n    end\n\n    # Subject\n    def self.subject\n      Thread.current[:accounts_processor_subject]\n    end\n\n    # @param dir [String] directory to load ruleset from\n    #\n    # Load rulesets from the specified folder\n    def self.load_ruleset(dir)\n      Dir.glob(\"#{